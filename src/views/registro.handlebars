<form class="row gy-2 gx-3 align-items-center">
    <div class="col-auto">
        <label for="inputNombre" class="form-label">Nombre</label>
        <input type="text" id="FirstName" class="form-control" id="nombre" placeholder="Ingrese Nombre">
    </div>
    <div class="col-auto">
        <label for="inputNombre" class="form-label">Apellido</label>
        <input type="text" id="LastName" class="form-control" id="nombre" placeholder="Ingrese Apellido">
    </div>
    <div class="col-auto">
        <label for="inputEmail4" class="form-label">Mail</label>
        <input type="email" class="form-control" id="email" placeholder="Ingrese correo electr칩nico">
    </div>
